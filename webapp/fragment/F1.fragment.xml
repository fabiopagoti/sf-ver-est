<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="{i18n>f1_title}">
		<content>
			<List mode="SingleSelectMaster" items="{/Plants}">
				<items>
					<StandardListItem title="{Id}" description="{Name}" />
				</items>
			</List>
		</content>
		<buttons>
			<Button text="{i18n>f1_button_confirm}" press=".onConfirmPlant"/>
			<Button text="{i18n>f1_button_cancel}" press=".onCancelPlant"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>